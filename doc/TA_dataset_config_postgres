    <import>
        <entity datasource="conn_cloudml_db" name="dataset">
            <query>
                <![CDATA[
                SELECT native_speaker, instructor, course, summer_regular, class_size, TA_score from ta_dataset t
                ]]>
            </query>
            <field column="t.native_speaker" name="native_speaker" type="integer" />
            <field column="t.instructor" name="instructor" type="integer" />
            <field column="t.course" name="course" type="integer" />
            <field column="t.summer_regular" name="summer_regular" type="integer" />
            <field column="t.class_size" name="class_size" type="integer" />
            <field column="t.TA_score" name="TA_score" type="integer" />
        </entity>
    </import>
