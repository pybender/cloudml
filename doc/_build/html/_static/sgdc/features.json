{
  "schema-name": "example",
  "classifier": {
    "type": "stochastic gradient descent classifier",
    "params": {
      "n_iter": 20,
      "shuffle": true,
      "loss": "log"
    }
  },
  "features":[
    {
      "name":"sex",
      "type":"map",
      "params": {
        "mappings":{
          "I": 0,
          "M": 1,
          "F": 2
        }
      },
      "is-target-variable":true,
      "is-required":true
    },
    {
      "name":"square",
      "type":"float"
    },
    {
      "name":"diameter",
      "type":"float"
    },
    {
      "name":"whole_weight",
      "type":"float"
    },
    {
      "name":"shucked_weight",
      "type":"float"
    },
    {
      "name":"rings",
      "type":"int"
    }
  ]
}
